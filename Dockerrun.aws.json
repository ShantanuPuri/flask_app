{
    "AWSEBDockerrunVersion": 1,
    "containerDefinitions": [
      {
        "name": "flask_app",
        "image": "shantanupuri26/flask_app2-flask_app:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "containerPort": 5000,
            "hostPort": 5000
          }
        ]
      },
      {
        "name": "mysql_db",
        "image": "mysql:8.0",
        "essential": true,
        "memory": 256,
        "environment": [
          {
            "name": "MYSQL_ROOT_PASSWORD",
            "value": "root_password"
          },
          {
            "name": "MYSQL_DATABASE",
            "value": "flask_app_db"
          },
          {
            "name": "MYSQL_USER",
            "value": "flask_user"
          },
          {
            "name": "MYSQL_PASSWORD",
            "value": "*snuggy_14"
          }
        ],
        "portMappings": [
          {
            "containerPort": 3306,
            "hostPort": 3306
          }
        ]
      }
    ]
  }
  